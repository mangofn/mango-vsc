{
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"]
    ],
    "autoClosingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"]
    ],
    "surroundingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"]
    ],
    "wordPattern": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b|#[0-9a-fA-F]{3,6}\\b",
    "indentationRules": {
        "increaseIndentPattern": "\\{\\s*$",
        "decreaseIndentPattern": "^\\s*\\}.*$"
    }
}
